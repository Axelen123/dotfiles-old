{
    "languageserver": {
        "golang": {
            "command": "gopls",
            "rootPatterns": ["go.mod", ".vim/", ".git/", ".hg/"],
            "filetypes": ["go"]
        },
        "svelte": {
            "command": "svelteserver",
            "rootPatterns": ["package.json"],
            "args": ["--stdio"],
            "filetypes": ["svelte"]
        }
    }
}
